{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "react/prop-types": 0,
        "react/jsx-filename-extension": 0,
        "indent": [ "error", 4, { "SwitchCase": 1 } ],
        "react/jsx-indent-props": 0,
        "react/jsx-indent": 0
    }
}